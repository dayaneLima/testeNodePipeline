{
  "name": "serverrestapi",
  "version": "1.0.0",
  "description": "Testes Automatizados de API",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test jest --testTimeout=10000",
    "test:unit": "cross-env NODE_ENV=test jest  --testTimeout=10000 --testNamePattern=UnitTest",
    "test:integration": "cross-env NODE_ENV=test jest  --testTimeout=10000 --testNamePattern=IntegrationTest",
    "cypress:run": "./node_modules/.bin/cypress run -P tests",
    "dev": "./node_modules/.bin/nodemon --watch 'src/' --exec \"ts-node src/server.ts\" -e ts",
    "prod": "node dist/server.js",
    "build": "./node_modules/.bin/tsc --build"
  },
  "author": "Dayane",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "cypress": "^12.4.1",
    "jest": "^29.4.2",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1"
  }
}
